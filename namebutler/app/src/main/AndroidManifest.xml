<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.namebutler">

    <dist:module dist:instant="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TopActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".CharacterDetailsActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".CharacterMakeActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".CharacterCreateActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".CharacterCreateDoneActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".PartyActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".BattleActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".StrategyActivity"
            android:label="キャラ作成" >
        </activity>
        <activity
            android:name=".BattleResultActivity"
            android:label="キャラ作成" >
        </activity>
    </application>
</manifest>
